{
  "name": "qier-player",
  "version": "2.0.0-alpha",
  "description": "A multifunctional and beautiful React-based web video player.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node scripts/server",
    "clean": "rimraf ./dist",
    "build-ts": "tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist/es && tsccss -o ./dist/es && copyfiles -f ./src/assets/* ./dist/es/assets",
    "build-css": "node-sass -r ./src -o ./dist/es",
    "es-build": "npm run build-ts && npm run build-css",
    "umd-build": "cross-env NODE_ENV=production webpack --config ./scripts/config/webpack.prod.js",
    "build": "npm run clean && npm run es-build && npm run umd-build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "npm run lint-eslint && npm run lint-stylelint",
    "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint-stylelint": "stylelint --config .stylelintrc.js src/**/*.{less,css,scss}"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 9",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vortesnail/qier-player.git"
  },
  "keywords": [
    "h5-player",
    "video-player",
    "video",
    "player",
    "react-player",
    "react-components",
    "html5-video-player"
  ],
  "author": {
    "name": "vortesnail",
    "url": "https://github.com/vortesnail",
    "email": "1091331061@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vortesnail/qier-player/issues"
  },
  "homepage": "https://github.com/vortesnail/qier-player",
  "devDependencies": {
    "@babel/core": "~7.13.10",
    "@babel/plugin-transform-runtime": "~7.13.10",
    "@babel/preset-env": "~7.13.10",
    "@babel/preset-react": "~7.12.13",
    "@babel/preset-typescript": "~7.13.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "~8.2.2",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "detect-port-alt": "~1.1.6",
    "error-overlay-webpack-plugin": "^0.4.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.3.8",
    "ip": "^1.1.5",
    "is-root": "~2.1.0",
    "lint-staged": "^10.5.4",
    "node-sass": "^4.14.1",
    "postcss": "^8.2.8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "rimraf": "~3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^5.1.1",
    "tsccss": "0.0.3",
    "tscpaths": "0.0.9",
    "typescript": "~4.2.3",
    "webpack": "^5.24.4",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "~7.13.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
